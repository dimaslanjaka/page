{
  "name": "wmi-page",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "ci": "yarn install --production=true --immutable --immutable-cache --check-cache --frozen-lockfile --check-files",
    "prod:update": "cd page && npx npm-check-updates -u && npm install --omit=dev --production",
    "prod": "cd page && npm install --omit=dev --production",
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && npx npm-check-updates -u",
    "serve": "node server.js",
    "build": "gulp --series build page:commit",
    "post-install": "node postinstall.js --yarn",
    "server-dump": "node server.js &> server.log"
  },
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com> (http://webmanajemen.com)",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/dimaslanjaka/page.git"
  },
  "packageManager": "yarn@3.5.0",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gulp": "^4.0.2",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.10.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "workspaces": [
    "packages/safelinkify",
    "page-source"
  ]
}

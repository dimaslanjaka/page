{% set canonical = "https://www.webmanajemen.com/page/moment-timezone.html" %}
{% set lang = "en" %}
{% extends '_layout.njk' %}

{% block title %}Moment Timezone Playground{% endblock %}

{% block head %}
<meta name="description" content="Moment Timezone Online Playground For Free. Support custom format pattern">
<link rel="alternate" href="https://codepen.io/dimaslanjaka/pen/LYegjaV" />
<!-- defer stylesheet -->
<link rel="preload" as="style" href="/page/assets/css/moment-timezone.css"
  onload="this.onload=null;this.rel='stylesheet'" />
<noscript>
  <link rel="stylesheet" href="/page/assets/css/moment-timezone.css">
</noscript>
<!--
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js"></script>
<script src="/page/node_modules/moment/min/moment-with-locales.min.js"></script>
<script src="/page/node_modules/moment-timezone/builds/moment-timezone-with-data.min.js"></script>
-->
<script>
  window['adsense-onload'] = true;
  window['adsense-debug'] = false;
</script>

{% endblock head %}

{% block content %}
<div class="text-center">
  <h1><a href="https://www.webmanajemen.com/page/moment-timezone.html" target="_blank">Moment Timezone
      Playground</a>
  </h1>
  <small class="text-muted">Parse and display dates in any timezone.</small>
</div>
<section>{% include "./partials/moment-timezone/date-input.njk" %}</section>
<section>{% include "./partials/moment-timezone/clock.njk" %}</section>
<section>{% include "./partials/moment-timezone/snippet.njk" %}</section>
<section>{% include "./partials/moment-timezone/full-timezone-list.njk" %}</section>
{% endblock content %}

{% block endbody %}
<script src="/page/assets/js/moment-timezone.js" async></script>
{% endblock endbody %}

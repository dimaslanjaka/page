    "import": "bluebird"
  },
  "moment": {
    "import": "moment",
    "dependOn": "promise"
  },
  "mtz": {
    "import": "moment-timezone",
    "dependOn": ["moment", "promise"]
  },
  "tslib": {
    "import": "tslib"
  },
  "cryptojs": {
    "import": "crypto-js"
  },
  "lodash": {
    "import": "lodash",
    "dependOn": "promise"
  },
  "firebase": {
    "import": ["firebase/app", "firebase/auth"],
    "dependOn": ["promise", "tslib"]
  },
  "bundle": {
    "import": "./src/index.js",
    "dependOn": ["promise", "mtz", "firebase"]
  }
}
